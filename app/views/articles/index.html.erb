<div>
  <h1>Wikinimous</h1>
  <%= link_to "New Article", new_article_path %>
</div>

<div>
  <ul>
    <% @articles.each do |article| %>
      <li>
        <%= link_to article_path(article) do %>
          <%= article.title %>
        <% end %>
        <%= article.content  %>
      </li>
    <% end %>
  </ul>
</div>
